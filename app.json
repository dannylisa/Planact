{
  "expo": {
    "name": "Planact",
    "slug": "Planact",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.insiders.planact",
      "buildNumber": "0.0.0",
      "usesIcloudStorage": true,
      "icon": "./src/assets/icons/icon.png"
    },
    "android": {
      "package": "com.insiders.planact",
      "versionCode": 0,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
